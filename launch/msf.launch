<?xml version="1.0"?>
<launch>
	<node pkg="rqt_gui" type="rqt_gui" name="msf_gcs" output="screen"
		clear_params="true"
		args="--perspective-file $(find px4_gcs)/msf.perspective">
	</node>

	<node pkg="px4_gcs" type="px4_gcs" name="px4_gcs" output="screen" clear_params="true">
		<param name="platform" type="string" value="pixhawk" />
		<param name="navigation" type="string" value="msf" />
	</node>

	<!--node pkg="rviz" type="rviz" name="rviz_gcs" output="screen" 
		args="-d $(find px4_gcs)/rviz_gcs.rviz">
	</node>
	
	<node pkg="rostopic" type="rostopic" name="rviz_init" output="screen" 
		args="pub -1 -f $(find px4_gcs)/rviz_init.yaml /tf tf2_msgs/TFMessage">
	</node-->

</launch>
